.wrap {
  display: flex;
  height: calc(100vh - 80rpx);
}
.wrap .left_muen {
  width: 200rpx;
  overflow: auto;
}
.wrap .left_muen .muen_item {
  display: flex;
  justify-content: center;
  align-items: center;
  color: #666;
  background-color: #f4f4f4;
  padding: 30rpx 0;
  font-size: 26rpx;
}
.wrap .left_muen .muen_item.active {
  border-left: 2rpx solid #eb4450;
  color: #eb4450;
  background-color: #fff;
}
.wrap .right_list {
  flex: 1;
  overflow: auto;
}
.wrap .right_list .muen_item_list .item_title {
  display: flex;
  height: 80rpx;
  justify-content: center;
  align-items: center;
  font-size: 26rpx;
}
.wrap .right_list .muen_item_list .title_list {
  display: flex;
  flex-wrap: wrap;
}
.wrap .right_list .muen_item_list .title_list .item2_list {
  width: 33.33%;
  text-align: center;
  color: #666;
  font-size: 26rpx;
}
.wrap .right_list .muen_item_list .title_list .item2_list image {
  width: 50%;
}
